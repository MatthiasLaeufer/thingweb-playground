{
    "id": "urn:dev:wot:com:example:servient:lamp",
    "name": "MyLampThing",
    "description": "Warning TD since interactions key is not allowed",
        "securityDefinitions": {
            "basic_sc": {
                "scheme": "basic",
                "in": "header"
            }
        },
        "security": ["basic_sc"],
    "interactions": {
        "properties": {
            "status": {
                "type": "string",
                "forms": [{
                    "href": "https://mylamp.example.com/status"
                }],
                "security": [{
                    "scheme": "basic",
                    "in": "header"
                }]
            }
        },
        "actions": {
            "toggle": {
                "forms": [{
                    "href": "https://mylamp.example.com/toggle"
                }],
                "security": [{
                    "scheme": "nosec"
                }]
            }
        },
        "events": {
            "overheating": {
                "data": {
                    "type": "string"
                },
                "forms": [{
                    "href": "https://mylamp.example.com/oh",
                    "subProtocol": "longpoll"
                }],
                "security": [{
                    "scheme": "cert",
                    "identity": "abcd123"
                }]
            }
        }
    }
}